{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "core/prompting/schemas/voxis_phrase_generation_output.json",
    "title": "Voxis Phrase Generation Output",
    "type": "object",
    "additionalProperties": false,
    "required": ["phrase"],
    "properties": {
      "phrase": {
        "type": "string",
        "minLength": 3,
        "maxLength": 120,
        "pattern": "^(?:[A-Za-z][A-Za-z'\\-]*\\s){5}[A-Za-z][A-Za-z'\\-]*$"
      }
    }
  }
  